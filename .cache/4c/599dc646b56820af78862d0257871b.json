%;\r\n  padding: 20px;\r\n  text-align: center;\r\n  border-bottom: none;\r\n  li {\r\n    display: inline-block;\r\n    list-style: none;\r\n    margin-right: 30px;\r\n    padding: 5px 10px;\r\n  }\r\n}\r\n\r\n.tab {\r\n  text-decoration: none;\r\n  color: $tab-color;\r\n  cursor: pointer;\r\n  line-height: 1.6;\r\n  padding: 0.25rem;\r\n  border: none;\r\n  background-color: inherit;\r\n  font-size: 1.5rem;\r\n  font-weight: 600;\r\n\r\n  &.active {\r\n    border-bottom: 4px solid $active-tab;\r\n    transition: 0.25s ease;\r\n  }\r\n  &:hover {\r\n    color: lighten($tab-color, 20%);\r\n  }\r\n}\r\n\r\n@media (max-width: 600px) {\r\n  .nav-tabs li {\r\n    display: block;\r\n  }\r\n}\r\n\r\n.tab-content {\r\n  display: none;\r\n  padding-bottom: 20px;\r\n  &.show {\r\n    display: block;\r\n  }\r\n}\r\n\r\n// Tab 1\r\n\r\n#date-form {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n#weather-date {\r\n  margin: 10px 0;\r\n  max-width: 200px;\r\n}\r\n \r\n\r\n// Tab 3\r\n.heat-index-section {\r\n  max-width: 800px;\r\n  padding: 1rem 1.25rem;\r\n  margin: 0 auto;\r\n}\r\n\r\n.heat-index-wrapper {\r\n  width: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n#heat-index-form {\r\n  margin: 1rem 0;\r\n  min-width: 350px;\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr 1fr;\r\n  align-items: center;\r\n  gap: 10px 0;\r\n  margin: 20px 0;\r\n  font-size: 1.2rem;\r\n  ::placeholder {\r\n    color: $neutral;\r\n  }\r\n  label {\r\n    margin-bottom: 0.5rem;\r\n    font-size: 1rem;\r\n  }\r\n  select {\r\n    padding: 10px 15px;\r\n    border-radius: 5px;\r\n    width: 75px;\r\n  }\r\n}\r\n\r\n.heat-index-input {\r\n  width: 100px;\r\n  padding: 0.75rem 0.5rem;\r\n  border: none;\r\n  border-radius: 3px;\r\n  text-align: right;\r\n  font-size: 1rem;\r\n}\r\n\r\n.btn {\r\n  width: 100px;\r\n  color: #fff;\r\n  background-color: $btn;\r\n  font-weight: 600;\r\n  font-size: 0.9rem;\r\n  cursor: pointer;\r\n  border-radius: 3px;\r\n  padding: 0.6rem 0.5rem;\r\n  border: none;\r\n  &:hover {\r\n    background-color: lighten($btn, 5%);\r\n    color: #fff;\r\n  }\r\n}\r\n\r\n.heat-index-result {\r\n  min-width: 350px;\r\n  margin-top: 50px;\r\n  padding: 20px 20px 20px 15px;\r\n  border: 1px solid #fff;\r\n  border-radius: 3px;\r\n  .result {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    h3 {\r\n      margin-top: 5px;\r\n    }\r\n    #heat-index {\r\n      margin: 0 20px 0 10px;\r\n      font-weight: 600;\r\n    }\r\n  }\r\n}\r\n\r\n#degrees-result {\r\n  padding: 10px 15px;\r\n  border-radius: 5px;\r\n  width: 75px;\r\n  font-size: 1.2rem;\r\n}\r\n\r\n.error-message {\r\n  margin-top: 20px;\r\n  color: red;\r\n}\r\n\r\nh4 {\r\n  font-size: 1.2rem;\r\n  font-weight: 600;\r\n  margin-bottom: 30px;\r\n  margin-top: 50px;\r\n  text-align: center;\r\n}\r\n\r\n.recent-results-container {\r\n  width: 100%;\r\n  margin-top: 30px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.recent-results {\r\n  padding: 0;\r\n}\r\n\r\n.recent-result {\r\n  background-color: #fff;\r\n  border-radius: 20px;\r\n  padding: 5px 10px;\r\n  min-width: 100px;\r\n  text-align: center;\r\n  list-style: none;\r\n  margin-bottom: 10px;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"75486977acbc0d867368918ff3717b67","cacheData":{"env":{}}}